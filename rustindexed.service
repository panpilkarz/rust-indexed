[Unit]
Description=Selend
After=syslog.target

[Service]
Type=simple
WorkingDirectory=/root/rust-indexed.git
ExecStart=/root/.cargo/bin/cargo run --release
# Requires systemd version 211 or newer
Restart=always
KillSignal=SIGQUIT
StandardError=syslog
NotifyAccess=all

[Install]
WantedBy=multi-user.target
